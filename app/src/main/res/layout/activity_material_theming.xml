<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.material.MaterialThemingActivity"
    tools:ignore="HardcodedText">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <include layout="@layout/layout_material_theming_text" />

        <include layout="@layout/layout_material_theming_button" />

        <include layout="@layout/layout_material_theming_clip" />

        <include layout="@layout/layout_material_theming_selection" />

        <include layout="@layout/layout_material_theming_text_field" />

        <include layout="@layout/layout_material_theming_tab" />

        <include layout="@layout/layout_material_theming_card" />


    </LinearLayout>
</androidx.core.widget.NestedScrollView>